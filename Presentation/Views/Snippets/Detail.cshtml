@model Cogensoft.SnippetManager.Application.Snippets.Queries.GetSnippetDetail.SnippetDetailModel

@{
    ViewBag.Title = "Snippet";
}

<h2>Snippet</h2>
<table>
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Date</th>
        <td>@Model.Date.ToString("d")</td>
    </tr>
    <tr>
        <th>Customer</th>
        <td>@Model.Description</td>
    </tr>
</table>

